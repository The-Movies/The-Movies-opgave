<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <RootNamespace>The_movie_egen</RootNamespace>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="UI\View\Pictures\DeStoreFilm.png" />
    <None Remove="UI\View\Pictures\NameLogo.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="UI\View\Pictures\DeStoreFilm.png" />
    <Resource Include="UI\View\Pictures\NameLogo.png" />
  </ItemGroup>

  <ItemGroup>
    <Page Update="UI\View\Themes\Movies.Dark.xaml">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Page>
  </ItemGroup>

</Project>
